  "scripts": {
    "start": "run-p start:**",
    "dev": "yarn start",
    "frontend": "gatsby develop",
    "start:app": "yarn frontend",
    "start:lambda": "netlify-lambda serve src/functions",
    "build": "gatsby build && netlify-lambda build src/functions",
    "build:app": "gatsby build",
    "build:lambda": "netlify-lambda build src/functions",
    "serve": "gatsby serve",
    "test": "yarn run lint:css && yarn run lint:js && yarn run check-pretty",
    "setup": "node ./bin/setup.js",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:css": "stylelint 'src/**/*.js'",
    "check-pretty": "prettier --list-different {,src/**/}*.{js,jsx,json,css,scss}",
    "format": "prettier --write \"src/**/*.js\"",
    "clean": "gatsby clean",
    "pushDevToMaster": "git co master && git pull && git merge dev && git push && git co dev",
    "netlify-lambda:install": "netlify-lambda install src/functions"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-gatsby-standard": "^2.2.0",
